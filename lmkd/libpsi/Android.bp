cc_library_headers {
    name: "libpsi_headers",
    export_include_dirs: ["include"],
}

cc_library {
    name: "libpsi",
    srcs: ["psi.c"],
    shared_libs: [
        "liblog"
    ],
    header_libs: [
        "libpsi_headers",
    ],
    export_header_lib_headers: [
        "libpsi_headers",
    ],
    cflags: [
        "-Wall",
        "-Werror",
    ],
}
