#on post-fs-data
on property:vold.decrypt=trigger_restart_framework
    write /proc/bootprof "start trusty secure storage"
    mkdir /data/trusty 0775 system system
    start storageproxyd

# Start storageproxyd Daemon
service storageproxyd /system/bin/storageproxyd -d /dev/trusty-ipc-dev0 -p /data/trusty -r /dev/block/mmcblk0rpmb
    user root
    group root
    class late_start
    oneshot
